{
    "AWSEBDockerrunVersion": 1,
    "volumes": [
      {
        "name": "app",
        "host": {
          "sourcePath": "/app"
        }
      }
    ],
    "containerDefinitions": [
      {
        "name": "app",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 4000
          }
        ]
      }
    ]
  }